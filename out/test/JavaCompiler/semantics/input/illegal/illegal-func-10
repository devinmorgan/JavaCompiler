// foo() is not guaranteed to return a value
int foo() {
    if (true) {
        if (true) {
            if (true) {
                return 1;
            }
            else {
                if (true) {
                    if (true) {
                        // missing return stmt
                    } else { return 1; }
                } else { return 1; }
            }
        } else { return 1; }
    } else { return 1; }
}